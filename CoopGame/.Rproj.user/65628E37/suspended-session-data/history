getSigns<-function(cbm,n){
numberOfRows=dim(cbm)[1];
cardOfTs=apply(cbm,1,function(x){sum(x[1:n])});
cardOfK=cardOfTs[numberOfRows];
signs=apply(cardOfTs,1,function(x){(-1)^(cardOfK-x)});
return(signs);
}
