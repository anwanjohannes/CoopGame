getCorrespondingRows<-function(n,index,bm){
N=2^n-1;
ix=!as.vector(bm[index,1:n]);
pm=matrix(ix,nrow=index,ncol=n,byrow = TRUE);
#AND binary and pattern matrix
rm=bm[1:s_ix,1:n]&pm
#Get indices where the special players are not involved
indices=!apply(rm,1,function(x){any(x)})
indices=c(indices,rep(FALSE,N-s_ix))
#Select according rows
return(indices);
}
