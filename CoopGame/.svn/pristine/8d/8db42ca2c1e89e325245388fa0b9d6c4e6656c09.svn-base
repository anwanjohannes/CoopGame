#' @title AllocationProperty - Virtual class for allocation concepts
#' @description Virtual class for allocation concepts, inherits methods and slots from class
#' \linkS4class{CoopGameProperty}.
#' @include CoopGameProperty.R
#' @template slot/x
#' @exportClass AllocationProperty
setClass(
  "AllocationProperty",
  representation(x="numeric","VIRTUAL"),
  contains = "CoopGameProperty"
)

setMethod(
  f="initialize",
  signature = "AllocationProperty",
  definition =  function(.Object, ..., x=x) {
    .Object@x<-x
    .Object<-methods::callNextMethod(.Object, ...)
    
    # methods::validObject(.Object)
    return(.Object)
  }
)



#' @rdname determineProperty-methods
#' @aliases determineProperty,AllocationProperty-method
setGeneric(
  "determineProperty",
  function(.Object){
    standardGeneric("determineProperty")
  }
)


