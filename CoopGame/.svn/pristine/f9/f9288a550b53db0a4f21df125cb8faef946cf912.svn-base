#' @title AllocationProperty - Virtual class for point valued solution concepts
#' @description Virtual class for point valued solution concepts
#' @noRd
#' @name AllocationProperty
#' @include CoopGameProperty.R
#' @exportClass AllocationProperty

setClass(
  "AllocationProperty",
  representation(x="numeric","VIRTUAL"),
  contains = "CoopGameProperty"
)

setMethod(
  f="initialize",
  signature = "AllocationProperty",
  definition =  function(.Object, ..., x=x) {
    .Object@x<-x
    .Object<-methods::callNextMethod(.Object, ...)
    
    # methods::validObject(.Object)
    return(.Object)
  }
)



#' @title determineProperty
#' @description determineProperty
#' @name determineProperty
#' @noRd
#' @template author/JA
#' @template author/JS
#' @exportMethod determineProperty
setGeneric(
  "determineProperty",
  function(.Object){
    standardGeneric("determineProperty")
  }
)


