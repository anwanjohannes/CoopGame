boolSkip=F




test_that("Check 63.1 - testing init of S4 class LPMatrix" ,{
  if(boolSkip){
    skip("Test was skipped")
  }
  coeffMat<-createBitMatrix(n=3,1:7)
  lpMatrix<-new("LPMatrix",matrix=coeffMat)
  expect_equal(lpMatrix@matrix,coeffMat)
})

test_that("Check 63.2 - testing wrapper function LPRows" ,{
  if(boolSkip){
    skip("Test was skipped")
  }
  
  coeffMat<-createBitMatrix(n=3,1:7)
  lpMatrix<-LPMatrix(coeffMat)
  expect_equal(lpMatrix@matrix,coeffMat)
})

test_that("Check 63.3 - testing validator of class LPRows when invalid" ,{
  if(boolSkip){
    skip("Test was skipped")
  }

  coeffMat<-createBitMatrix(n=3,1:7)

  lpMatrix<-LPMatrix(coeffMat)
  lpMatrix@matrix<-lpMatrix@matrix[1:6,]
  expect_equal("FALSE",validObject(lpMatrix,test=TRUE))
})

test_that("Check 63.4 - testing validator of class LPRows when valid" ,{
  if(boolSkip){
    skip("Test was skipped")
  }
  
  coeffMat<-createBitMatrix(n=3,1:7)
  
  lpMatrix<-LPMatrix(coeffMat)
  lpMatrix@matrix[,"cVal"]<-11:17
  expect_true(validObject(lpMatrix,test=TRUE))
})

test_that("Check 63.5 - testing getter and setter of class LPMatrix" ,{
  if(boolSkip){
    skip("Test was skipped")
  }
  
  
  coeffMat<-createBitMatrix(n=3,1:7)
  
  lpMatrix<-new("LPMatrix")
  
  setMatrixLP(lpMatrix)<-coeffMat

  
  expect_equal(getMatrixLP(lpMatrix),coeffMat)

})
